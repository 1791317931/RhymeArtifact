.submitting-body {
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  padding: 40rpx 0 0 0;
}

@keyframes image-rotate {
  from {
    transform: rotate(0);
  }
  to {
    transform: rotate(360deg);
  }
}

.submitting-body image {
  width: 66rpx;
  height: 66rpx;
  animation: image-rotate 1s linear infinite;
}

.submitting-body view {
  margin-top: 10rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(35,35,35,1);
  line-height:34rpx;
  letter-spacing:1rpx;
}

.save-modal-body, .update-beat-modal-body, .akbl-modal-body {
  width: 614rpx;
  height: 608rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
}

.input-placeholder {
  color: #999;
  font-size:30rpx;
  font-family:PingFangSC-Medium;
}

.save-modal-body .modal-title,
.update-beat-modal-body .modal-title,
.akbl-modal-body .modal-title {
  text-align: center;
  margin: 20rpx 0 76rpx;
  font-size:34rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(35,35,35,1);
  line-height:48rpx;
  letter-spacing:1rpx;
}

.update-beat-modal-body .modal-title, .akbl-modal-body .modal-title {
  margin-bottom: 357rpx;
}

.update-beat-modal-body .modal-body, .akbl-modal-body .modal-body {
  width: 396rpx;
  text-align: center;
  position: absolute;
  top: 218rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size:38rpx;
  font-family:PingFangSC-Semibold;
  font-weight:600;
  color:rgba(35,35,35,1);
  line-height:53rpx;
  letter-spacing:1rpx;
}

.save-modal-body .row {
  padding: 0 30rpx 0 130rpx;
  position: relative;
}

.save-modal-body .row:not(:first-child) {
  margin-top: 40rpx;
}

.save-modal-body .row view {
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(35,35,35,1);
  letter-spacing:1rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.save-modal-body .row input {
  height: 78rpx;
  padding: 0 30rpx;
  background:rgba(250,250,250,1);
  border-radius:6rpx;
  border:1px solid rgba(218,218,218,1);
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(35,35,35,1);
  letter-spacing:1rpx;
}

.save-modal-body .btn-container {
  padding: 0 40rpx;
  margin-top: 85rpx;
}

.update-beat-modal-body .btn-container, .akbl-modal-body .btn-container {
  padding: 0 40rpx;
}

.save-modal-body .btn-container button,
.update-beat-modal-body .btn-container button,
.akbl-modal-body .btn-container button {
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius:6px;
  border:2rpx solid rgba(0,0,0,1);
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(0,0,0,1);
  letter-spacing:1rpx;
}

.save-modal-body .btn-container button:last-child,
.update-beat-modal-body .btn-container button:last-child,
.akbl-beat-modal-body .btn-container button:last-child {
  background:rgba(6,6,6,1);
  color:rgba(255,255,255,1);
}

.create-music-container {
  background: rgb(46, 46, 46);
  height: 100%;
}

.scale-container-wrapper {
  padding: 0 0 0 165rpx;
}

.scale-container-wrapper .scale-container {
  display: flex;
}

.scale-container-wrapper .scale-container > view {
  flex: 1;
  height: 36rpx;
  position: relative;
  font-size:18rpx;
  font-family:PingFangSC-Regular;
  color:rgba(255,255,255,1);
  text-align: center;
}

.scale-container-wrapper .scale-container > view:before {
  content: '';
  width:1rpx;
  height:6rpx;
  background:rgba(151,151,151,1);
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translateX(-50%);
}

.scale-container-wrapper .pointer-container {
  position: relative;
}

.scale-container-wrapper .pointer-container image {
  width: 21rpx;
  height: 360rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 100000;
}

.scale-container-wrapper .pointer-container image.single {
  height: 180rpx;
}

.combine-container .top-container {
  height: 141rpx;
  position: relative;
}

.beat-title, .freestyle-title {
  width: 156rpx;
  height:120rpx;
  line-height: 120rpx;
  text-align: center;
  padding: 0 7rpx;
  background:rgba(97,98,97,1);
  font-size:24rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  position:absolute;
  left:0;
  top:50%;
  transform:translateY(-50%);
}

.track-container {
  position: absolute;
  top: 50%;
  right: 0;
  left: 165rpx;
  transform: translateY(-50%);
  border:1rpx solid rgba(201,227,255,1);
  border-radius: 0 0 0 13rpx;
  overflow: hidden;
}

.track-container:before {
  content: '';
  height: 2rpx;
  background:rgba(116,156,198,1);
  position: absolute;
  left: 0;
  right: 0;
  bottom: 44rpx;
  z-index: 3;
}

.track-container .head-container {
  height:30rpx;
  line-height:30rpx;
  padding: 0 0 0 13rpx;
  background:rgba(201,227,255,1);
  font-size:18rpx;
  font-family:PingFangSC-Regular;
  color:rgba(19,62,107,1);
}

.track-container > .high-container, .track-container > .low-container {
  height: 45rpx;
  background:rgba(49,88,130,1);
}

.track-container > .voice-container {
  width: 100%;
  height: 88rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 1;
}

.track-container > .voice-container image {
  width: 20%;
  height: 88rpx;
}

.track-container > .voice-mask-container {
  width: 100%;
  height: 88rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  background:rgba(49,88,130,1);
  z-index: 2;
}

.track-container .high-container {
  border-bottom: 1rpx solid rgba(116,156,198,1);
}

.track-container .recoding-container {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 4;
  background:rgba(201,49,41, .5);
}

.combine-container .bottom-container {
  line-height: 40rpx;
  font-size:18rpx;
  font-family:PingFangSC-Regular;
  color:rgba(255,255,255,1);
  padding: 0 0 0 165rpx;
  position: relative;
  background:rgba(51,51,51,1);
}

.combine-container .bottom-container .node {
  position: absolute;
  width: 165rpx;
  text-align: center;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}

.combine-container .bottom-container .time-start {
  width: 50%;
  display: inline-block;
}

.combine-container .bottom-container .time-end {
  width: 50%;
  display: inline-block;
  padding-right: 14rpx;
  text-align: right;
}

.center-btn-container {
  margin-top: 35rpx;
  padding: 0 60rpx;

}

.center-btn-container > view {
  float: left;
  position: relative;
}

.center-btn-container > view button {
  width:294rpx;
  height:98rpx;
  background:linear-gradient(180deg,rgba(70,69,71,1) 0%,rgba(105,105,105,1) 7%,rgba(100,100,100,1) 26%,rgba(67,66,65,1) 100%);
  box-shadow:0rpx 1rpx 2rpx 0rpx rgba(0,0,0,0.5);
  border-radius:14rpx;
  border:1rpx solid rgba(24,24,24,1);
  font-size:36rpx;
  font-family:PingFangSC-Light;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:98rpx;
  letter-spacing:1rpx;
}

.center-btn-container > view image {
  width:294rpx;
  height:98rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.center-btn-container > view:last-child {
  float: right;
}

.freestyle-detail-container {
  margin-top: 148rpx;
  text-align: center;
}

.freestyle-detail-container .title {
  font-size:42rpx;
  padding: 0 30rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:59rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.freestyle-detail-container .tip {
  margin-top: 40rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(86,91,98,1);
  line-height:33rpx;
}

.bottom-btn-container {
  width: 550rpx;
  margin: 125rpx 0 0 100rpx;
  position: relative;
  text-align: center;
}

.bottom-btn-container .record-state {
  width: 160rpx;
  height: 160rpx;
}

.bottom-btn-container > view {
  position: absolute;
  bottom: 0;
}

.bottom-btn-container > view image {
  width: 90rpx;
  height: 90rpx;
}

.bottom-btn-container > view > view {
  line-height:30rpx;
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  color:rgba(164,164,164,1);
}

.bottom-btn-container .replay-container {
  left: 0;
}

.bottom-btn-container .complete-container {
  right: 0;
}

.record-tip {
  margin-top: 10rpx;
  font-size:22rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(164,164,164,1);
  line-height:30rpx;
  text-align: center;
}