page {
  background: #fff;
}

.wrapper {
  padding: 50rpx 40rpx 30rpx;
}

.list .item {
  display: inline-block;
  width:320rpx;
  line-height:101rpx;
  background:rgba(246,247,249,1);
  border-radius:51rpx;
  font-size:34rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(30,30,30,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 20rpx;
  text-align: center;
}

.list .item.active {
  color: #fff;
  background:rgba(9,201,137,1);
}

.list .item:nth-child(2n+2) {
  margin-left: 30rpx;
}

.list .item:nth-child(n+3) {
  margin-top: 30rpx;
}

.submit {
  margin: 90rpx auto 0;
  width:606rpx;
  line-height:100rpx;
  background:rgba(9,201,137,1);
  border-radius:55rpx;
  text-align: center;
  font-size:34rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
}